Private Sub changeTurn()
    If player = "Y" Then
        WhoseTurn.Caption = "Computer's       Turn"
        WhoseTurn.BackColor = &HFF& 'red color
        player = "R"
    Else
        WhoseTurn.Caption = "  Player 1's        Turn"
        WhoseTurn.BackColor = &HFFFF& 'yellow color
        player = "Y"
    End If
End Sub
Function isMoveLegal(index) As Boolean
    Dim row As Integer, col As Integer
    col = index Mod MAX_SIZE
    row = (index - col) / MAX_SIZE
    If gamePos(row, col) = "" And player = "Y" Then
        isMoveLegal = True
    Else
        isMoveLegal = False
    End If
End Function

Function isMovePossible(index As Integer, choice As Integer, opponent As String) As Boolean
    Dim row As Integer, col As Integer
    Dim i As Integer, r As Integer, c As Integer
    Dim count As Integer
    Dim movePossible As Boolean
    
    Dim up As Integer, down As Integer
    Dim left As Integer, right As Integer
    Dim up_left As Integer, down_left As Integer
    Dim up_right As Integer, down_right As Integer
    
    curr_max_moves = 0
    
    col = index Mod MAX_SIZE
    row = (index - col) / MAX_SIZE
            
    isMovePossible = False
    movePossible = False
            
    'UP DIRECTION CHECKING
    count = 0
    r = row - 1
    c = col
    If (row >= 2) Then
        While (r >= 1 And gamePos(r, c) = opponent)
            r = r - 1
            count = count + 1
        Wend
        up = count
        If (r >= 0 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                If choice = MAKE_MOVE Then
                    For i = 0 To count
                        gamePos(row - i, col) = player
                        If player = "Y" Then
                            Digits(MAX_SIZE * (row - i) + col).BackColor = &HFFFF& 'yellow
                        Else
                            Digits(MAX_SIZE * (row - i) + col).BackColor = &HFF& 'red
                        End If
                    Next
                    changeNo_of_Sqrs player, count
                Else
                    curr_max_moves = curr_max_moves + count
                End If

                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    'DOWN DIRECTION CHECKING
    count = 0
    r = row + 1
    c = col
    If (row <= MAX_SIZE - 3) Then
        While (r <= MAX_SIZE - 2 And gamePos(r, c) = opponent)
            r = r + 1
            count = count + 1
        Wend
        down = count
        If (r <= MAX_SIZE - 1 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                If choice = MAKE_MOVE Then
                    For i = 0 To count
                        gamePos(row + i, col) = player
                        If player = "Y" Then
                            Digits(MAX_SIZE * (row + i) + col).BackColor = &HFFFF& 'yellow
                        Else
                            Digits(MAX_SIZE * (row + i) + col).BackColor = &HFF& 'red
                        End If
                    Next
                    changeNo_of_Sqrs player, count
                Else
                    curr_max_moves = curr_max_moves + count
                End If
                
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    
    'LEFT DIRECTION CHECKING
    count = 0
    r = row
    c = col - 1
    If (col >= 2) Then
        While (c >= 1 And gamePos(r, c) = opponent)
            c = c - 1
            count = count + 1
        Wend
        left = count
        If (c >= 0 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                If choice = MAKE_MOVE Then
                    For i = 0 To count
                        gamePos(row, col - i) = player
                        If player = "Y" Then
                            Digits(MAX_SIZE * row + col - i).BackColor = &HFFFF&  'yellow
                        Else
                            Digits(MAX_SIZE * row + col - i).BackColor = &HFF& 'red
                        End If
                    Next
                    changeNo_of_Sqrs player, count
                Else
                    curr_max_moves = curr_max_moves + count
                End If
                
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    
    'RIGHT DIRECTION CHECKING
    count = 0
    r = row
    c = col + 1
    If (col <= MAX_SIZE - 3) Then
        While (c <= MAX_SIZE - 2 And gamePos(r, c) = opponent)
            c = c + 1
            count = count + 1
        Wend
        right = count
        If (c <= MAX_SIZE - 1 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                If choice = MAKE_MOVE Then
                    For i = 0 To count
                        gamePos(row, col + i) = player
                        If player = "Y" Then
                            Digits(MAX_SIZE * row + col + i).BackColor = &HFFFF& 'yellow
                        Else
                            Digits(MAX_SIZE * row + col + i).BackColor = &HFF& 'red
                        End If
                    Next
                    changeNo_of_Sqrs player, count
                Else
                    curr_max_moves = curr_max_moves + count
                End If
                
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    
    'UP LEFT DIRECTION CHECKING
    count = 0
    r = row - 1
    c = col - 1
    If (row >= 2 And col >= 2) Then
        While (r >= 1 And c >= 1 And gamePos(r, c) = opponent)
            r = r - 1
            c = c - 1
            count = count + 1
        Wend
        up_left = count
        If (r >= 0 And c >= 0 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                If choice = MAKE_MOVE Then
                    For i = 0 To count
                        gamePos(row - i, col - i) = player
                        If player = "Y" Then
                            Digits(MAX_SIZE * (row - i) + col - i).BackColor = &HFFFF& 'yellow
                        Else
                            Digits(MAX_SIZE * (row - i) + col - i).BackColor = &HFF& 'red
                        End If
                    Next
                    changeNo_of_Sqrs player, count
                Else
                    curr_max_moves = curr_max_moves + count
                End If
                
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If

    
    'DOWN LEFT DIRECTION CHECKING
    count = 0
    r = row + 1
    c = col - 1
    If (row <= MAX_SIZE - 3 And col >= 2) Then
        While (r <= MAX_SIZE - 2 And c >= 1 And gamePos(r, c) = opponent)
            r = r + 1
            c = c - 1
            count = count + 1
        Wend
        down_left = count
        If (r <= MAX_SIZE - 1 And c >= 0 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                If choice = MAKE_MOVE Then
                    For i = 0 To count
                        gamePos(row + i, col - i) = player
                        If player = "Y" Then
                            Digits(MAX_SIZE * (row + i) + col - i).BackColor = &HFFFF& 'yellow
                        Else
                            Digits(MAX_SIZE * (row + i) + col - i).BackColor = &HFF& 'red
                        End If
                    Next
                    changeNo_of_Sqrs player, count
                Else
                    curr_max_moves = curr_max_moves + count
                End If
                
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    
    'UP RIGHT DIRECTION CHECKING
    count = 0
    r = row - 1
    c = col + 1
    If (row >= 2 And col <= MAX_SIZE - 3) Then
        While (r >= 1 And c <= MAX_SIZE - 2 And gamePos(r, c) = opponent)
            r = r - 1
            c = c + 1
            count = count + 1
        Wend
        up_right = count
        If (r >= 0 And c <= MAX_SIZE - 1 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                If choice = MAKE_MOVE Then
                    For i = 0 To count
                        gamePos(row - i, col + i) = player
                        If player = "Y" Then
                            Digits(MAX_SIZE * (row - i) + col + i).BackColor = &HFFFF& 'yellow
                        Else
                            Digits(MAX_SIZE * (row - i) + col + i).BackColor = &HFF& 'red
                        End If
                    Next
                    changeNo_of_Sqrs player, count
                Else
                    curr_max_moves = curr_max_moves + count
                End If
                
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    
    'DOWN RIGHT DIRECTION CHECKING
    count = 0
    r = row + 1
    c = col + 1
    If (row <= MAX_SIZE - 3 And col <= MAX_SIZE - 3) Then
        While (r <= MAX_SIZE - 2 And c <= MAX_SIZE - 2 And gamePos(r, c) = opponent)
            r = r + 1
            c = c + 1
            count = count + 1
        Wend
        down_right = count
        If (r <= MAX_SIZE - 1 And c <= MAX_SIZE - 1 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                If choice = MAKE_MOVE Then
                    For i = 0 To count
                        gamePos(row + i, col + i) = player
                        If player = "Y" Then
                            Digits(MAX_SIZE * (row + i) + col + i).BackColor = &HFFFF& 'yellow
                        Else
                            Digits(MAX_SIZE * (row + i) + col + i).BackColor = &HFF& 'red
                        End If
                    Next
                    changeNo_of_Sqrs player, count
                Else
                    curr_max_moves = curr_max_moves + count
                End If
                
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    'MsgBox "UP = " & up & " DOWN = " & down & " LEFT = " & left & " RIGHT = " & right
    'MsgBox "UP LEFT = " & up_left & " DOWN LEFT = " & down_left & " UP RIGHT = " & up_right & " DOWN RIGHT = " & down_right
    If movePossible And choice = MAKE_MOVE Then
        If player = "Y" Then
            yellow_sqrs = yellow_sqrs + 1
        Else
            red_sqrs = red_sqrs + 1
        End If
    End If
End Function
Function changeNo_of_Sqrs(player As String, count As Integer)
    'MsgBox count
    If player = "Y" Then
        yellow_sqrs = yellow_sqrs + count
        red_sqrs = red_sqrs - count
    Else
        red_sqrs = red_sqrs + count
        yellow_sqrs = yellow_sqrs - count
    End If
    
End Function
