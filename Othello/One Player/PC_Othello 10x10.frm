Attribute VB_Name = "Form1"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Dim player As String
Dim gamePos(15, 15) As String
Dim MAX_SIZE As Integer
Dim curr_max_moves As Integer, prev_max_moves As Integer
Dim best_move As Integer
Dim CHECK_MOVES As Integer, MAKE_MOVE As Integer

Dim yellow_sqrs As Integer, red_sqrs As Integer
Dim j As Integer, k As Integer

Private Sub Digits_Click(index As Integer)
    If isMoveLegal(index) Then
        If isMovePossible(index, MAKE_MOVE, "R") Then
            YellowSqr.Caption = "    " & yellow_sqrs
            RedSqr.Caption = "    " & red_sqrs
            YellowSqr.Refresh
            RedSqr.Refresh
            
            'change turn to computer
            changeTurn
            
            WhoseTurn.Refresh
            computer_plays
            YellowSqr.Caption = "    " & yellow_sqrs
            RedSqr.Caption = "    " & red_sqrs
            YellowSqr.Refresh
            RedSqr.Refresh
            
            changeTurn
        End If
    End If
    If Not isGameOver Then
        'check if atleast a move is possilble or not, either for PC or player
        If Not isMovePossibleFor("Y") Then
            'check if any move possible for PC
            If Not isMovePossibleFor("R") Then
                'game is over because no moves possible for any of the player
                MsgBox "No moves are possible for both players. Game is Over!", , "Othello"
                If yellow_sqrs > red_sqrs Then
                MsgBox "YELLOW Player Wins!!", , "Othello"
                ElseIf (yellow_sqrs < red_sqrs) Then
                    MsgBox "Red Player Wins!!", , "Othello"
                Else
                    MsgBox "It's a DRAW!!", , "Othello"
                End If
            Else 'if a move is possible for PC
                'change turn to computer
                changeTurn
                'computer plays untill there is no valid move possible for player and game is not over
                While (Not isMovePossibleFor("Y") And Not isGameOver)
                    MsgBox "No Move Possible for Yellow Player! Turn passes to the Computer", , "Othello"
                    WhoseTurn.Refresh
                    computer_plays
                    YellowSqr.Caption = "    " & yellow_sqrs
                    RedSqr.Caption = "    " & red_sqrs
                    YellowSqr.Refresh
                    RedSqr.Refresh
                Wend
                'change turn to player
                changeTurn
                If isGameOver Then
                    'End Sub
                End If
            End If
        End If
    End If
End Sub
Function isGameOver() As Boolean
    If yellow_sqrs + red_sqrs = MAX_SIZE * MAX_SIZE Or yellow_sqrs = 0 Or red_sqrs = 0 Then
        isGameOver = True
        If yellow_sqrs > red_sqrs Then
            MsgBox "Game Over! YELLOW Player Wins!!", , "Othello"
        ElseIf (yellow_sqrs < red_sqrs) Then
            MsgBox "Game Over! Red Player Wins!!", , "Othello"
        Else
            MsgBox "Game Over! It's a DRAW!!", , "Othello"
        End If
    Else
        isGameOver = False
    End If
End Function

Function isMovePossibleFor(curr_player As String) As Boolean
    Dim r As Integer, c As Integer
    Dim move_cnt As Integer
    Dim curr_opponent As String
    
    If curr_player = "Y" Then
        curr_opponent = "R"
    Else
        curr_opponent = "Y"
    End If
    
    move_cnt = 0
    For r = 0 To MAX_SIZE - 1
        For c = 0 To MAX_SIZE - 1
            k = MAX_SIZE * r + c
            If gamePos(r, c) = "" Then
                If isMovePossible(k, CHECK_MOVES, curr_opponent) Then
                    move_cnt = move_cnt + 1
                End If
            End If
        Next
    Next
    If move_cnt = 0 Then
        isMovePossibleFor = False
    Else
        isMovePossibleFor = True
    End If
    
End Function
Private Sub computer_plays()
    Dim r As Integer, c As Integer
    'evaluate each possible move
    For k = 0 To ((MAX_SIZE * MAX_SIZE) - 1)
        c = k Mod MAX_SIZE
        r = (k - c) / MAX_SIZE
        If gamePos(r, c) = "" Then
            If isMovePossible(k, CHECK_MOVES, "Y") Then
                If curr_max_moves > prev_max_moves Then
                    prev_max_moves = curr_max_moves
                    best_move = k
                End If
            End If
        End If
    Next
    
    'play in any of the outer most cells if possible
    '1) play in top row if possible
    r = 0   'top row
    For k = 0 To MAX_SIZE - 1
        c = k
        If gamePos(r, c) = "" Then
            If isMovePossible(MAX_SIZE * r + c, CHECK_MOVES, "Y") Then
                'If curr_max_moves > prev_max_moves Then
                    best_move = MAX_SIZE * r + c
                'End If
            End If
        End If
    Next
    
    '2) play in extreme left column if possible
    c = 0   'extreme left column
    For k = 0 To MAX_SIZE - 1
        r = k
        If gamePos(r, c) = "" Then
            If isMovePossible(MAX_SIZE * r + c, CHECK_MOVES, "Y") Then
                'If curr_max_moves > prev_max_moves Then
                    best_move = MAX_SIZE * r + c
                'End If
            End If
        End If
    Next
    
    '3) play in bottom most row if possible
    r = MAX_SIZE - 1 'bottom most row
    For k = 0 To MAX_SIZE - 1
        c = k
        If gamePos(r, c) = "" Then
            If isMovePossible(MAX_SIZE * r + c, CHECK_MOVES, "Y") Then
                'If curr_max_moves > prev_max_moves Then
                    best_move = MAX_SIZE * r + c
                'End If
            End If
        End If
    Next
    
    '4) play in extreme right column if possible
    c = MAX_SIZE - 1 'extreme right column
    For k = 0 To MAX_SIZE - 1
        r = k
        If gamePos(r, c) = "" Then
            If isMovePossible(MAX_SIZE * r + c, CHECK_MOVES, "Y") Then
                'If curr_max_moves > prev_max_moves Then
                    best_move = MAX_SIZE * r + c
                'End If
            End If
        End If
    Next
    
            
    'play at any of the corners if possible
    c = 0
    r = 0
    If gamePos(r, c) = "" And isMovePossible(0, CHECK_MOVES, "Y") Then
        best_move = 0
    End If
    c = MAX_SIZE - 1
    r = 0
    If gamePos(r, c) = "" And isMovePossible(MAX_SIZE - 1, CHECK_MOVES, "Y") Then
        best_move = MAX_SIZE - 1
    End If
    c = 0
    r = MAX_SIZE - 1
    If gamePos(r, c) = "" And isMovePossible((MAX_SIZE * MAX_SIZE - 1) - (MAX_SIZE - 1), CHECK_MOVES, "Y") Then
        best_move = (MAX_SIZE * MAX_SIZE - 1) - (MAX_SIZE - 1)
    End If
    c = MAX_SIZE - 1
    r = MAX_SIZE - 1
    If gamePos(r, c) = "" And isMovePossible(MAX_SIZE * MAX_SIZE - 1, CHECK_MOVES, "Y") Then
        best_move = MAX_SIZE * MAX_SIZE - 1
    End If


    If best_move >= 0 Then
        'MsgBox best_move
        prev_max_moves = 0
        isMovePossible best_move, MAKE_MOVE, "Y"
        c = best_move Mod MAX_SIZE
        r = (best_move - c) / MAX_SIZE
        'show where computer played
        showPC_move (best_move)
        'MsgBox "I played at " & r + 1 & "th row and " & c + 1 & "th column"
        Digits(best_move).BackColor = &HFF& 'red color
        best_move = -1
    Else
        MsgBox "No Moves Possible for Computer. Turn Passes to Yellow Player!", , "Othello"
    End If
End Sub
Private Sub showPC_move(best_move As Integer)
    'show where computer played
    'Digits(best_move).BackColor = &HC00000   'blue color
    Digits(best_move).BackColor = &HC0&
    
    Digits(best_move).Refresh
    Dim xyz As Integer, delay As Double
    For xyz = 0 To 1
        'Digits(best_move).BackColor = &HC00000    'blue color
        Digits(best_move).BackColor = &HC0&
        
        Digits(best_move).Refresh
        For delay = 0 To 3000000
        Next
        Digits(best_move).BackColor = &HFF& 'red color
        Digits(best_move).Refresh
        For delay = 0 To 3000000
        Next
    Next
End Sub

