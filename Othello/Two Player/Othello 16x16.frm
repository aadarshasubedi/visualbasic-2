Attribute VB_Name = "Form1"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Dim player As String
Dim gamePos(15, 15) As String
Dim MAX_SIZE As Integer
Dim yellow_sqrs As Integer, red_sqrs As Integer
Dim i As Integer, j As Integer

Private Sub Digits_Click(Index As Integer)
    If isMoveLegal(Index) Then
        If isMovePossible(Index) Then
            YellowSqr.Caption = "    " & yellow_sqrs
            RedSqr.Caption = "    " & red_sqrs
            changeTurn
        End If
    End If
    If isGameOver Then
        If yellow_sqrs > red_sqrs Then
            MsgBox "Game Over! YELLOW Player Wins!!", , "Othello"
        ElseIf (yellow_sqrs < red_sqrs) Then
            MsgBox "Game Over! Red Player Wins!!", , "Othello"
        Else
            MsgBox "Game Over! It's a DRAW!!", , "Othello"
        End If
        'NewGame_Click
    End If
End Sub
Private Sub Exit_Click()
    End
End Sub
Private Sub Form_Load()
    MAX_SIZE = 16
    NewGame_Click
End Sub

Private Sub Help_Click()
    MsgBox "Please read the 'readme.doc file. This file contains the detailed description of the game.", , "O T H E L L O"
End Sub
Private Sub NewGame_Click()
    For i = 0 To MAX_SIZE * MAX_SIZE - 1
        Digits(i).BackColor = &H8000000F 'default color
    Next
    For i = 0 To MAX_SIZE - 1
        For j = 0 To MAX_SIZE - 1
            gamePos(i, j) = ""
        Next
    Next
    yellow_sqrs = 2
    red_sqrs = 2
    YellowSqr.Caption = "    " & yellow_sqrs
    RedSqr.Caption = "    " & red_sqrs
    Digits(119).BackColor = &HFFFF& 'yellow color
    Digits(136).BackColor = &HFFFF& 'yellow color
    Digits(120).BackColor = &HFF& 'red color
    Digits(135).BackColor = &HFF& 'red color
    gamePos(7, 7) = "Y"
    gamePos(8, 8) = "Y"
    gamePos(7, 8) = "R"
    gamePos(8, 7) = "R"
    WhoseTurn.Caption = "  Player 1's        Turn"
    WhoseTurn.BackColor = &HFFFF& 'yellow color
    player = "Y"
End Sub
Private Sub changeTurn()
    If player = "Y" Then
        'gamePos((index - (index Mod 16)) / 16, index Mod 16) = "Y"
        WhoseTurn.Caption = "  Player 2's        Turn"
        WhoseTurn.BackColor = &HFF& 'red color
        player = "R"
        'Digits(index).BackColor = &HFFFF& 'yellow color
    Else
        'gamePos((index - (index Mod 16)) / 16, index Mod 16) = "R"
        WhoseTurn.Caption = "  Player 1's        Turn"
        WhoseTurn.BackColor = &HFFFF& 'yellow color
        player = "Y"
        'Digits(index).BackColor = &HFF& 'red color
    End If
End Sub
Function isMoveLegal(Index) As Boolean
    Dim row As Integer, col As Integer
    col = Index Mod MAX_SIZE
    row = (Index - col) / MAX_SIZE
    If gamePos(row, col) = "" Then
        isMoveLegal = True
    Else
        isMoveLegal = False
    End If
End Function
Function isMovePossible(Index) As Boolean
    Dim row As Integer, col As Integer
    Dim i As Integer, r As Integer, c As Integer
    Dim count As Integer
    Dim opponent As String
    Dim movePossible As Boolean
    
    Dim up As Integer, down As Integer
    Dim left As Integer, right As Integer
    Dim up_left As Integer, down_left As Integer
    Dim up_right As Integer, down_right As Integer
    
    col = Index Mod MAX_SIZE
    row = (Index - col) / MAX_SIZE
    If player = "Y" Then
        opponent = "R"
    Else
        opponent = "Y"
    End If
        
    isMovePossible = False
    movePossible = False
            
    'UP DIRECTION CHECKING
    count = 0
    r = row - 1
    c = col
    If (row >= 2) Then
        While (r >= 1 And gamePos(r, c) = opponent)
            r = r - 1
            count = count + 1
        Wend
        up = count
        If (r >= 0 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                For i = 0 To count
                    gamePos(row - i, col) = player
                    If player = "Y" Then
                        Digits(MAX_SIZE * (row - i) + col).BackColor = &HFFFF& 'yellow
                    Else
                        Digits(MAX_SIZE * (row - i) + col).BackColor = &HFF& 'red
                    End If
                Next
                changeNo_of_Sqrs player, count
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    'DOWN DIRECTION CHECKING
    count = 0
    r = row + 1
    c = col
    If (row <= MAX_SIZE - 3) Then
        While (r <= MAX_SIZE - 2 And gamePos(r, c) = opponent)
            r = r + 1
            count = count + 1
        Wend
        down = count
        If (r <= MAX_SIZE - 1 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                For i = 0 To count
                    gamePos(row + i, col) = player
                    If player = "Y" Then
                        Digits(MAX_SIZE * (row + i) + col).BackColor = &HFFFF& 'yellow
                    Else
                        Digits(MAX_SIZE * (row + i) + col).BackColor = &HFF& 'red
                    End If
                Next
                changeNo_of_Sqrs player, count
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    
    'LEFT DIRECTION CHECKING
    count = 0
    r = row
    c = col - 1
    If (col >= 2) Then
        While (c >= 1 And gamePos(r, c) = opponent)
            c = c - 1
            count = count + 1
        Wend
        left = count
        If (c >= 0 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                For i = 0 To count
                    gamePos(row, col - i) = player
                    If player = "Y" Then
                        Digits(MAX_SIZE * row + col - i).BackColor = &HFFFF&  'yellow
                    Else
                        Digits(MAX_SIZE * row + col - i).BackColor = &HFF& 'red
                    End If
                Next
                changeNo_of_Sqrs player, count
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    
    'RIGHT DIRECTION CHECKING
    count = 0
    r = row
    c = col + 1
    If (col <= MAX_SIZE - 3) Then
        While (c <= MAX_SIZE - 2 And gamePos(r, c) = opponent)
            c = c + 1
            count = count + 1
        Wend
        right = count
        If (c <= MAX_SIZE - 1 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                For i = 0 To count
                    gamePos(row, col + i) = player
                    If player = "Y" Then
                        Digits(MAX_SIZE * row + col + i).BackColor = &HFFFF& 'yellow
                    Else
                        Digits(MAX_SIZE * row + col + i).BackColor = &HFF& 'red
                    End If
                Next
                changeNo_of_Sqrs player, count
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    
    'UP LEFT DIRECTION CHECKING
    count = 0
    r = row - 1
    c = col - 1
    If (row >= 2 And col >= 2) Then
        While (r >= 1 And c >= 1 And gamePos(r, c) = opponent)
            r = r - 1
            c = c - 1
            count = count + 1
        Wend
        up_left = count
        If (r >= 0 And c >= 0 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                For i = 0 To count
                    gamePos(row - i, col - i) = player
                    If player = "Y" Then
                        Digits(MAX_SIZE * (row - i) + col - i).BackColor = &HFFFF& 'yellow
                    Else
                        Digits(MAX_SIZE * (row - i) + col - i).BackColor = &HFF& 'red
                    End If
                Next
                changeNo_of_Sqrs player, count
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If

    
    'DOWN LEFT DIRECTION CHECKING
    count = 0
    r = row + 1
    c = col - 1
    If (row <= MAX_SIZE - 3 And col >= 2) Then
        While (r <= MAX_SIZE - 2 And c >= 1 And gamePos(r, c) = opponent)
            r = r + 1
            c = c - 1
            count = count + 1
        Wend
        down_left = count
        If (r <= MAX_SIZE - 1 And c >= 0 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                For i = 0 To count
                    gamePos(row + i, col - i) = player
                    If player = "Y" Then
                        Digits(MAX_SIZE * (row + i) + col - i).BackColor = &HFFFF& 'yellow
                    Else
                        Digits(MAX_SIZE * (row + i) + col - i).BackColor = &HFF& 'red
                    End If
                Next
                changeNo_of_Sqrs player, count
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    
    'UP RIGHT DIRECTION CHECKING
    count = 0
    r = row - 1
    c = col + 1
    If (row >= 2 And col <= MAX_SIZE - 3) Then
        While (r >= 1 And c <= MAX_SIZE - 2 And gamePos(r, c) = opponent)
            r = r - 1
            c = c + 1
            count = count + 1
        Wend
        up_right = count
        If (r >= 0 And c <= MAX_SIZE - 1 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                For i = 0 To count
                    gamePos(row - i, col + i) = player
                    If player = "Y" Then
                        Digits(MAX_SIZE * (row - i) + col + i).BackColor = &HFFFF& 'yellow
                    Else
                        Digits(MAX_SIZE * (row - i) + col + i).BackColor = &HFF& 'red
                    End If
                Next
                changeNo_of_Sqrs player, count
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    
    
    'DOWN RIGHT DIRECTION CHECKING
    count = 0
    r = row + 1
    c = col + 1
    If (row <= MAX_SIZE - 3 And col <= MAX_SIZE - 3) Then
        While (r <= MAX_SIZE - 2 And c <= MAX_SIZE - 2 And gamePos(r, c) = opponent)
            r = r + 1
            c = c + 1
            count = count + 1
        Wend
        down_right = count
        If (r <= MAX_SIZE - 1 And c <= MAX_SIZE - 1 And count >= 1) Then
            If (gamePos(r, c) = player) Then
                For i = 0 To count
                    gamePos(row + i, col + i) = player
                    If player = "Y" Then
                        Digits(MAX_SIZE * (row + i) + col + i).BackColor = &HFFFF& 'yellow
                    Else
                        Digits(MAX_SIZE * (row + i) + col + i).BackColor = &HFF& 'red
                    End If
                Next
                changeNo_of_Sqrs player, count
                isMovePossible = True
                movePossible = True
            End If
        End If
    End If
    'MsgBox "UP = " & up & " DOWN = " & down & " LEFT = " & left & " RIGHT = " & right
    'MsgBox "UP LEFT = " & up_left & " DOWN LEFT = " & down_left & " UP RIGHT = " & up_right & " DOWN RIGHT = " & down_right
    If movePossible Then
        If player = "Y" Then
            yellow_sqrs = yellow_sqrs + 1
        Else
            red_sqrs = red_sqrs + 1
        End If
    End If
End Function

Function isGameOver() As Boolean
    If yellow_sqrs + red_sqrs = MAX_SIZE * MAX_SIZE Or yellow_sqrs = 0 Or red_sqrs = 0 Then
        isGameOver = True
    Else
        isGameOver = False
    End If
End Function

Function changeNo_of_Sqrs(player As String, count As Integer)
    'MsgBox count
    If player = "Y" Then
        yellow_sqrs = yellow_sqrs + count
        red_sqrs = red_sqrs - count
    Else
        red_sqrs = red_sqrs + count
        yellow_sqrs = yellow_sqrs - count
    End If
    
End Function

